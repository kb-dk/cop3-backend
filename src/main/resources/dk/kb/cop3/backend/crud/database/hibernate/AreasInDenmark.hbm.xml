<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- written by slu@kb.dk 14/10 2022 -->
<hibernate-mapping>                                   
    <class name="dk.kb.cop3.backend.migrate.hibernate.AreasInDenmark" table="AREAS_IN_DK">
        <id name="id">
            <column name="AREA_ID"/>
            <generator class="assigned" />
        </id>
        <property name="nameOfArea" type="string">
          <column name="NAME_OF_AREA" length="255"/>
        </property>
        <property name="polygon" type="dk.kb.cop3.backend.crud.database.type.JGeometryType">
          <column name="POLYGON_COL"/>
        </property>
    </class>
</hibernate-mapping>
